m1-get-cargo(r, c, d, p):
  task: get-cargo(c)
  pre: loc(r) == d && on-dock(p, d) && in-pile(c) == p && on-robot(r) == nil
  body:
    pickupCargo(r, c, p)
    
m1-put-cargo(r, c, d, p):
  task: put-cargo(c, p)
  pre: loc(r) == d && on-dock(p, d) && on-robot(r) == c
  body:
    putCargo(r, c, p)
    